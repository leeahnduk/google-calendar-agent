# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
STAGING_BUCKET=gs://your-project-staging-bucket

# Agent Configuration  
AGENT_DISPLAY_NAME=Meeting_Prep_Agent
AUTH_ID=meeting-prep-auth

# AI Models
ROOT_AGENT_MODEL=gemini-2.5-flash
SUB_AGENT_MODEL=gemini-2.5-flash

# Behavior
BRIEF_LEAD_MINUTES=30
HISTORICAL_LOOKBACK_DAYS=90

# Chat Integration Preferences
GOOGLE_CHAT_ENABLED=true
CHAT_INTEGRATION_PREFERENCE=both  # Options: "slack", "google_chat", or "both"

# Optional: Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret

# Optional: OAuth Credentials (if needed for custom setup)
# CLIENT_ID=your-oauth-client-id
# CLIENT_SECRET=your-oauth-client-secret